{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cricket": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/assets/docs/*"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/theme.css",
              "src/assets/css/flexslider.min.css",
              "src/assets/css/elegant-icons.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/font-cardo.css",
              "src/assets/css/font-helvetica.css",
              "src/assets/css/font-lato.css",
              "src/assets/css/font-opensans.css",
              "src/assets/css/font-roboto.css",
              "src/assets/css/font-titillium.css",
              "src/assets/css/ie9.css",
              "src/assets/css/custom.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/scripts/jquery.min.js",
              "src/assets/scripts/jquery.plugin.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/scripts/jquery.flexslider-min.js",
              "src/assets/scripts/smooth-scroll.min.js",
              "src/assets/scripts/skrollr.min.js",
              "src/assets/scripts/spectragram.min.js",
              "src/assets/scripts/scrollReveal.min.js",
              "src/assets/scripts/isotope.min.js",
              "src/assets/scripts/twitterFetcher_v10_min.js",
              "src/assets/scripts/lightbox.min.js",
              "src/assets/scripts/jquery.countdown.min.js",
              "src/assets/scripts/modernizr-2.6.2-respond-1.1.0.min.js",
              "src/assets/scripts/scripts.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cricket:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cricket:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cricket:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "src/assets/scripts/jquery.min.js",
              "src/assets/scripts/jquery.plugin.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/scripts/jquery.flexslider-min.js",
              "src/assets/scripts/smooth-scroll.min.js",
              "src/assets/scripts/skrollr.min.js",
              "src/assets/scripts/spectragram.min.js",
              "src/assets/scripts/scrollReveal.min.js",
              "src/assets/scripts/isotope.min.js",
              "src/assets/scripts/twitterFetcher_v10_min.js",
              "src/assets/scripts/lightbox.min.js",
              "src/assets/scripts/jquery.countdown.min.js",
              "src/assets/scripts/modernizr-2.6.2-respond-1.1.0.min.js",
              "src/assets/scripts/scripts.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/theme.css",
              "src/assets/css/flexslider.min.css",
              "src/assets/css/elegant-icons.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/font-cardo.css",
              "src/assets/css/font-helvetica.css",
              "src/assets/css/font-lato.css",
              "src/assets/css/font-opensans.css",
              "src/assets/css/font-roboto.css",
              "src/assets/css/font-titillium.css",
              "src/assets/css/ie9.css",
              "src/assets/css/custom.css",
              "src/styles.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/assets/docs/*"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "cricket-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "cricket:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "cricket",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}