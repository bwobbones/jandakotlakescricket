{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cricket"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "sitemap.xml",
        "./assets/docs/*"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "assets/css/theme.css",
        "assets/css/flexslider.min.css",
        "assets/css/elegant-icons.min.css",
        "assets/css/font-awesome.min.css",
        "assets/css/font-cardo.css",
        "assets/css/font-helvetica.css",
        "assets/css/font-lato.css",
        "assets/css/font-opensans.css",
        "assets/css/font-roboto.css",
        "assets/css/font-titillium.css",
        "assets/css/ie9.css",
        "assets/css/custom.css",
        "styles.css"
      ],
      "scripts": [
        "assets/scripts/jquery.min.js",
        "assets/scripts/jquery.plugin.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "assets/scripts/jquery.flexslider-min.js",
        "assets/scripts/smooth-scroll.min.js",
        "assets/scripts/skrollr.min.js",
        "assets/scripts/spectragram.min.js",
        "assets/scripts/scrollReveal.min.js",
        "assets/scripts/isotope.min.js",
        "assets/scripts/twitterFetcher_v10_min.js",
        "assets/scripts/lightbox.min.js",
        "assets/scripts/jquery.countdown.min.js",
        "assets/scripts/modernizr-2.6.2-respond-1.1.0.min.js",
        "assets/scripts/scripts.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
